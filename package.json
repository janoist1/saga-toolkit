{
  "name": "saga-toolkit",
  "version": "2.2.0",
  "description": "An extension for redux-toolkit that allows sagas to resolve async thunk actions.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/janoist1/saga-toolkit.git"
  },
  "keywords": [
    "redux",
    "toolkit",
    "saga",
    "async",
    "thunk",
    "resolve"
  ],
  "author": "Istvan Jano",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/janoist1/saga-toolkit/issues"
  },
  "homepage": "https://github.com/janoist1/saga-toolkit#readme",
  "devDependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@types/node": "^20.0.0",
    "@types/redux-saga": "^0.10.5",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "redux": "^4.1.2",
    "redux-saga": "^1.2.3",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.53.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "redux-saga": "^1.0.0"
  }
}